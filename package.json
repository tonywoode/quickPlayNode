    "fs-extra": "^4.0.2",
    "ini": "^1.3.4",
    "levenshtein": "^1.0.5",
    "mkdirp": "^0.5.1",
    "pkg": "~4.4.9",
    "ramda": "^0.24.1",
    "replace-in-file": "^4.1.3",
    "sanctuary": "^2.0.0",
    "string-similarity": "^4.0.4",
    "trash-cli": "^1.4.0",
